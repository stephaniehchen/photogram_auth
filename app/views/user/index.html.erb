<div class="page-header">
  <h1>
    User
  <!--  <a href="/user/new" class="btn btn-lg btn-success">New User</a>-->
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Email</th>
        <th>Username</th>
      </tr>

      <% @user.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.username %></td>
        <td>
          <a href="/user/<%= user.id %>" class="btn btn-primary">Show</a>

        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
